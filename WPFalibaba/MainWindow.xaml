            </syncfusion:SfChart.PrimaryAxis>

            <syncfusion:SfChart.SecondaryAxis>
                <syncfusion:NumericalAxis Minimum="0" Maximum="60000" 
                    Interval="1" >
                    <syncfusion:NumericalAxis.Header>
                        <TextBlock Margin="10" Text="Milliseconds" FontSize="16" FontFamily="SegoeUI"/>
                    </syncfusion:NumericalAxis.Header>
                </syncfusion:NumericalAxis>
            </syncfusion:SfChart.SecondaryAxis>

            <syncfusion:FastLineBitmapSeries EnableAntiAliasing="False" Label="First" 
